<idea-plugin>
    <id>com.github.adebmbng.gorepogen</id>
    <name>go-repo-gen</name>
    <vendor>adebmbng</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">

    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="generate_func_method" class="com.github.adebmbng.gorepogen.GenerateFunction" text="com.github.adebmbng.gorepogen.GenerateFunction"
                description="Generate Go repo">
            <keyboard-shortcut keymap="$default" first-keystroke="shift ctrl Q"/>
        </action>
    </actions>

</idea-plugin>
